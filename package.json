{
  "name": "remix-portfolio",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production remix vite:build",
    "build:svg": "svgo -f ./app/assets/icons",
    "build:icons": "npx @svgr/cli --config-file ./svgr.config.cjs -- ./app/assets/icons",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "run-s lint:*",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx --color ./",
    "lint:ls": "npx @ls-lint/ls-lint",
    "lint:prettier": "npx prettier --check .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.10.3",
    "@remix-run/cloudflare-pages": "^2.10.3",
    "@remix-run/react": "^2.16.6",
    "cssnano": "^7.0.7",
    "date-fns": "^4.1.0",
    "eslint": "^8.57.0",
    "isbot": "^5.1.26",
    "postcss-extend-rule": "^4.0.0",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.8",
    "react-vertical-timeline-component": "^3.6.0",
    "recharts": "^2.15.3",
    "remix-utils": "^7.6.0",
    "svgo": "^3.3.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240718.0",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.1",
    "@ls-lint/ls-lint": "^2.3.0",
    "@remix-run/dev": "^2.10.3",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-vertical-timeline-component": "^3.3.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "autoprefixer": "^10.4.21",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-mixins": "^11.0.3",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.4",
    "terser": "^5.39.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^7.16.1",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "4.37.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
